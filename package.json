{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "dotenv": "^16.4.5",
    "expo": "^54.0.0",
    "expo-linking": "~8.0.11",
    "expo-status-bar": "~3.0.9",
    "formik": "^2.4.9",
    "graphql": "^16.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.83.1",
    "react-router-native": "^6.30.0",
    "rxjs": "^7.8.2",
    "web-streams-polyfill": "^4.2.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "babel-preset-expo": "~54.0.10",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-jest": "^29.12.1",
    "react-test-renderer": "^19.1.0",
    "react-refresh": "^0.18.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
